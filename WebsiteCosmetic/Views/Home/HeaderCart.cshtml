@model List<Models.Framework.OderDetail>
<li>
    <a href="#offcanvas-add-cart" class="offcanvas-toggle minicartLink ">
        <i class="icon-bag"></i>
        @if (Model.Count == 0)
        {
            <span class="item-count quantity-minicart">0</span>
        }
        else
        {
            <span class="item-count quantity-minicart">@Model.Count</span>
        }
    </a>
</li>
@section Script{
    <script src="/Assets/Font/js/Base1.js"></script>

}
